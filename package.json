{
  "name": "@nyknyc/wagmi-connector",
  "version": "0.1.0",
  "description": "NYKNYC 4337 Smart Wallet Connector for Wagmi",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "wagmi",
    "ethereum",
    "web3",
    "wallet",
    "connector",
    "4337",
    "account-abstraction",
    "nyknyc"
  ],
  "author": "NYKNYC",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nyknyc/wagmi-connector"
  },
  "peerDependencies": {
    "@wagmi/core": "^2.20.3",
    "viem": "^2.37.5"
  },
  "devDependencies": {
    "@types/node": "^20.19.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "sideEffects": false
}
