{
  "name": "@nyknyc/wagmi-connector",
  "version": "0.1.4",
  "description": "NYKNYC 4337 Smart Wallet Connector for Wagmi",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "wagmi",
    "ethereum",
    "web3",
    "wallet",
    "connector",
    "4337",
    "account-abstraction",
    "nyknyc"
  ],
  "author": "NYKNYC",
  "license": "MIT",
  "homepage": "https://nyknyc.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nyknyc-app/wagmi-connector"
  },
  "peerDependencies": {
    "@wagmi/core": "^2.22.0",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@wagmi/core": "^3.0.0",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "viem": "^2.43.3",
    "vitest": "^3.2.4"
  },
  "sideEffects": false
}
